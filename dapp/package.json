{
  "name": "dapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "clsx": "^1.1.1",
    "query-string": "7",
    "react": "^17.0.1",
    "react-d3-tree": "^2.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5",
    "relay": "link:./../relay",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "yarn --cwd ../relay install --production=false --frozen-lockfile --non-interactive && yarn --cwd ../relay build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "yarn --cwd ../relay install --production=false --frozen-lockfile --non-interactive && yarn --cwd ../relay build",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path .eslintignore .",
    "lint-fix": "eslint --fix --ext .ts,.tsx,.js,.jsx --ignore-path .eslintignore ."
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix --ext .ts,.tsx,.js,.jsx --ignore-path .eslintignore ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-google": "^0.14.0",
    "husky": "v4.3.8",
    "lint-staged": "^10.5.4"
  }
}
